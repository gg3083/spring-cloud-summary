<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gg.backend.mapper1.UserMapper">

    <insert id="insert">
        INSERT INTO
          t_user
            (user_name,user_pwd)
          VALUES
            ( #{userName},#{userPwd})
    </insert>

    <select id="selectAll" resultType="com.gg.backend.domain.User">
        select * , pk_user_id as id  from t_user
    </select>

</mapper>